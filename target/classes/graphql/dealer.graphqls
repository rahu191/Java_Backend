type Dealer {
	id: ID!
	dName: String!
	email: String
	manufacturer: Manufacturer
}

extend type Query {
	findAllDealers: [Dealer]!
	countDealers: Long!
}

extend type Mutation {
	createDealer(input: DealerWrapper): Dealer!
	updateDealer(id: ID!, dName: String, email: String): Dealer!
	deleteDealer(id: ID!): Boolean
}

input DealerWrapper{
dName: String!
email: String
manufacturer: Manufacture!
}

input Manufacture {
    mName: String
    vin: Int
}